@model IEnumerable<MachineViewModel>
@{
    ViewData["Title"] = "Machines";
}


<html>
    <body>
        <div class="contianer">
            <h1>Hello World Welcome to machine page</h1>
            <h2>Machine List</h2>
        @foreach(var machines in Model)
        {
                <div>
                    <h2>Machine Name: @machines.Name</h2>
                    <p>Machine Ip Address: @machines.Ip</p>
                </div>
        }
        </div>
    </body>
</html>